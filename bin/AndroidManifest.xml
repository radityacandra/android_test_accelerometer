<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.spcb2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainAccelerometer"
            android:label="@string/app_name" >
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Main_Menu"
            android:label="@string/title_activity_main__menu" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Pilihan_Burn_Cal"
            android:label="@string/title_activity_pilihan__burn__cal" >
        </activity>
        <activity
            android:name=".Survival_Mode"
            android:label="@string/title_activity_survival__mode" >
        </activity>
        <activity
            android:name=".High_Score"
            android:label="@string/title_activity_high__score" >
        </activity>
        <activity
            android:name=".Challenge_Mode"
            android:label="@string/title_activity_challenge__mode" >
        </activity>
    </application>

    <uses-permission android:name="android.permission.READ_PHONE_STATE" >
    </uses-permission>

</manifest>
